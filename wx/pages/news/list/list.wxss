
#menus{
    background: #ffffff;
    margin-top:10px;
    height:auto;
    width:100%;
}
#menus .content{
    height:45px;
  border-bottom: 1rpx solid #E5E5E5;
  margin-left:30rpx;
}

#menus .content .view{
    float: left;
    width: 560rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}
#menus .content .view text{
    font-size:15px;
    line-height:45px;
    padding-left:0px;
}
#menus .content .right{
    height: 45px;
    float: right;
    width: 120rpx;
    margin-right:10px;
    line-height: 45px;
    color: gray;
    font-size: 13px;
}
